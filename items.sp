int GetWeaponIndexFromWeaponId(int weapon_id)
{
    for (int i = 0; i < sizeof(items_weapon_ids); i++)
    {
        if (items_weapon_ids[i] == weapon_id)
        {
            return i;
        }
    }

    return -1;
}

int GetRandomSkinFromWeaponId(int weapon_id)
{
    int weapon_index = GetWeaponIndexFromWeaponId(weapon_id);

    if (weapon_index == -1)
    {
        return 0;
    }

    int skin_count = items_weapon_skin_count[weapon_index];
    int skin_index = GetRandomInt(0, skin_count - 1);
    return items_weapon_skin_ids[weapon_index][skin_index];
}
